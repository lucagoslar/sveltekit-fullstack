{
	"name": "sveltekit-fullstack",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm-run-all --parallel vite:dev typesafe-i18n",
		"build": "typesafe-i18n --no-watch && vite build",
		"preview": "vite preview",
		"vite:dev": "vite dev",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"story:dev": "npm-run-all --parallel typesafe-i18n histoire:dev",
		"story:build": "npm-run-all --parallel typesafe-i18n histoire:build",
		"histoire:dev": "histoire dev",
		"histoire:build": "histoire build",
		"story:preview": "histoire preview",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@histoire/plugin-svelte": "^0.11.9",
		"@prisma/client": "^4.7.1",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"@trpc/server": "^10.4.3",
		"@types/accept-language-parser": "^1.5.3",
		"@typescript-eslint/eslint-plugin": "^5.45.1",
		"@typescript-eslint/parser": "^5.45.1",
		"accept-language-parser": "^1.5.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"histoire": "^0.11.9",
		"npm-run-all": "^4.1.5",
		"postcss-pxtorem": "^6.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.7.1",
		"sass": "^1.56.1",
		"sharp": "^0.31.2",
		"svelte": "^3.53.1",
		"svelte-check": "^2.10.1",
		"svelte-meta-tags": "^2.6.4",
		"svelte-preprocess": "^4.10.7",
		"svelte-preprocess-sass-alias-import": "^1.0.0",
		"trpc-sveltekit": "^3.0.8",
		"tslib": "^2.4.1",
		"typesafe-i18n": "^5.17.1",
		"typescript": "^4.9.3",
		"vite": "^3.2.5",
		"vite-imagetools": "^4.0.11",
		"vite-plugin-svelte-svg": "^2.0.2"
	},
	"type": "module",
	"dependencies": {
		"@trpc/client": "^10.4.3",
		"normalize.css": "^8.0.1"
	}
}
