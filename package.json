{
	"name": "sveltekit-fullstack",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"typesafe-i18n\" \"vite dev\"",
		"build": "typesafe-i18n --no-watch && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"seed": "ts-node-esm prisma/seed.ts"
	},
	"devDependencies": {
		"@babel/core": "^7.22.9",
		"@storybook/addon-actions": "^7.1.0",
		"@storybook/addon-essentials": "^7.1.0",
		"@storybook/addon-interactions": "^7.1.0",
		"@storybook/addon-links": "^7.1.0",
		"@storybook/addon-svelte-csf": "^3.0.4",
		"@storybook/blocks": "^7.1.0",
		"@storybook/builder-vite": "^7.1.0",
		"@storybook/cli": "^7.1.0",
		"@storybook/svelte": "^7.1.0",
		"@storybook/sveltekit": "^7.1.0",
		"@storybook/testing-library": "^0.2.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.22.3",
		"@types/accept-language-parser": "^1.5.3",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.1.0",
		"autoprefixer": "^10.4.14",
		"babel-loader": "^9.1.3",
		"concurrently": "^8.2.0",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-storybook": "^0.6.13",
		"eslint-plugin-svelte3": "^4.0.0",
		"imagetools-core": "^4.0.3",
		"postcss-pxtorem": "^6.0.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prisma": "^5.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.64.0",
		"storybook": "^7.1.0",
		"svelte": "^4.1.0",
		"svelte-check": "^3.4.6",
		"svelte-loader": "^3.1.9",
		"svelte-preprocess-sass-alias-import": "^1.0.0",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"vite": "^4.4.4",
		"vite-imagetools": "^5.0.5",
		"vite-plugin-svelte-svg": "^2.2.1"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^5.0.0",
		"@trpc/client": "^10.35.0",
		"@trpc/server": "^10.35.0",
		"accept-language-parser": "^1.5.0",
		"normalize.css": "^8.0.1",
		"svelte-meta-tags": "^2.8.0",
		"trpc-sveltekit": "^3.5.17",
		"ts-node": "^10.9.1",
		"typesafe-i18n": "^5.25.0",
		"zod": "^3.21.4"
	}
}
