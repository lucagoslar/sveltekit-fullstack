{
	"name": "sveltekit-fullstack",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"typesafe-i18n\" \"vite dev\"",
		"build": "typesafe-i18n --no-watch && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"seed": "ts-node-esm prisma/seed.ts"
	},
	"devDependencies": {
		"@babel/core": "^7.23.9",
		"@poppanator/sveltekit-svg": "^4.2.1",
		"@storybook/addon-actions": "^7.6.12",
		"@storybook/addon-essentials": "^7.6.12",
		"@storybook/addon-interactions": "^7.6.12",
		"@storybook/addon-links": "^7.6.12",
		"@storybook/addon-svelte-csf": "^4.1.0",
		"@storybook/blocks": "^7.6.12",
		"@storybook/builder-vite": "^7.6.12",
		"@storybook/cli": "^7.6.12",
		"@storybook/svelte": "^7.6.12",
		"@storybook/sveltekit": "^7.6.12",
		"@storybook/testing-library": "^0.2.2",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/enhanced-img": "^0.1.8",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/accept-language-parser": "^1.5.6",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"autoprefixer": "^10.4.17",
		"babel-loader": "^9.1.3",
		"concurrently": "^8.2.2",
		"dotenv": "^16.4.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-storybook": "^0.6.15",
		"eslint-plugin-svelte": "^2.35.1",
		"imagetools-core": "^6.0.4",
		"postcss-pxtorem": "^6.1.0",
		"prettier": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"prisma": "^5.9.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.70.0",
		"storybook": "^7.6.12",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"svelte-loader": "^3.1.9",
		"svelte-preprocess-sass-alias-import": "^1.0.0",
		"svgo": "^3.2.0",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.12"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^5.9.0",
		"@trpc/client": "^10.45.0",
