{
	"name": "sveltekit-fullstack",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"typesafe-i18n\" \"vite dev\"",
		"build": "typesafe-i18n --no-watch && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"seed": "ts-node-esm prisma/seed.ts"
	},
	"devDependencies": {
		"@babel/core": "^7.23.2",
		"@storybook/addon-actions": "^7.5.1",
		"@storybook/addon-essentials": "^7.5.1",
		"@storybook/addon-interactions": "^7.5.1",
		"@storybook/addon-links": "^7.5.1",
		"@storybook/addon-svelte-csf": "^4.0.9",
		"@storybook/blocks": "^7.5.1",
		"@storybook/builder-vite": "^7.5.1",
		"@storybook/cli": "^7.5.1",
		"@storybook/svelte": "^7.5.1",
		"@storybook/sveltekit": "^7.5.1",
		"@storybook/testing-library": "^0.2.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.27.1",
		"@types/accept-language-parser": "^1.5.5",
		"@typescript-eslint/eslint-plugin": "^6.9.0",
		"@typescript-eslint/parser": "^6.9.0",
		"autoprefixer": "^10.4.16",
		"babel-loader": "^9.1.3",
		"concurrently": "^8.2.2",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-storybook": "^0.6.15",
		"eslint-plugin-svelte3": "^4.0.0",
		"imagetools-core": "^5.1.0",
		"postcss-pxtorem": "^6.0.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prisma": "^5.5.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.69.5",
		"storybook": "^7.5.1",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"svelte-loader": "^3.1.9",
		"svelte-preprocess-sass-alias-import": "^1.0.0",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vite-imagetools": "^6.1.0",
		"vite-plugin-svelte-svg": "^2.3.0"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^5.5.2",
		"@trpc/client": "^10.43.0",
		"@trpc/server": "^10.43.0",
		"accept-language-parser": "^1.5.0",
		"normalize.css": "^8.0.1",
		"svelte-meta-tags": "^3.0.4",
		"trpc-sveltekit": "^3.5.22",
		"ts-node": "^10.9.1",
		"typesafe-i18n": "^5.26.2",
		"zod": "^3.22.4"
	}
}
